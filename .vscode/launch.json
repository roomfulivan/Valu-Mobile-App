{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Android",
			"program": "${workspaceRoot}/.vscode/launchReactNative.js",
			"type": "reactnative",
			"request": "launch",
			"platform": "android",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
		},
		{
			"name": "Debug iOS",
			"program": "${workspaceRoot}/.vscode/launchReactNative.js",
			"type": "reactnative",
			"request": "launch",
			"platform": "ios",
      "target": "iPhone 5s",
      "cwd": "${workspaceFolder}",
			"sourceMaps": true,
		}
	]
}